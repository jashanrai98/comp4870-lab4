@model IEnumerable<lab4_.Models.Province>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.provName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.provCode)
            </th>
           
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.provName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.provCode)
            </td>
            
            <td>
                <a asp-action="Edit" asp-route-id="@item.provCode">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.provCode">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.provCode">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
